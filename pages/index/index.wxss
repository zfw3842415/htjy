/*新闻列表*/
.block-newsList {
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
  padding: 0 30rpx;
  border-top: 1px solid #eee;
}

.block-newsList .news-item {
  display: inline-block;
  width: 100%;
  position: relative;
  border-bottom: 1px solid #eee;
  padding: 30rpx 0;
}

.block-newsList .news-item:last-child {
  border-bottom: 0;
}

.block-newsList .news-item .news-item-r {
  float: right;
  width: 165rpx;
  height: 120rpx;
  margin-left: 20rpx;
  border: 2rpx solid #f1f1f1;
}

.block-newsList .news-item .news-item-r image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
}

.block-newsList .news-item .news-item-l {
  min-height: 120rpx;
  position: relative;
  overflow: hidden;
}
.block-newsList .news-item .news-item-l .info-wrapper {
  max-height: 40px;
  overflow: hidden;
}
.block-newsList .news-item .news-item-l .title {
  font-size: 28rpx;
  color: #353535;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  margin: 0;
}

.block-newsList .news-item .news-item-l .desc {
  color: #a9a9a9;
  margin-top: 10rpx;
  font-size: 24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-height: 38rpx;
}

.block-newsList .news-item .news-item-l .time {
  font-size: 18rpx;
