/* 设置点点的样式 */
swiper .wx-swiper-dot {
  display: inline-flex;
  flex-direction: row;
  width: 36rpx;
  height: 10rpx;
  border-radius: 20%;
  background: #ffffff;
}

/* 当前点点的样式 */
swiper .wx-swiper-dot::before {
  content: '';
  width: 36rpx;
  height: 10rpx;
  border-radius: 20%;
  background: #ffffff;
}

swiper .wx-swiper-dot-active::before {
  width: 36rpx;
  height: 10rpx;
  background-color: #108DEE;
  border-radius: 20%;
}

